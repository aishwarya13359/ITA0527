
import cv2

# Read image
image = cv2.imread("C:/Program Files/python/WhatsApp Image 2026-02-27 at 2.12.24 PM.jpeg")

# Apply Gaussian Blur
blur = cv2.GaussianBlur(image, (7, 7), 0)

# Show images
cv2.imshow("Original Image", image)
cv2.imshow("Gaussian Blur Image", blur)

cv2.waitKey(0)
cv2.destroyAllWindows()
